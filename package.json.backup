    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "pinia-plugin-persistedstate": "^3.1.0",
    "pinyin": "3.0.0-alpha.5",
    "plop": "^3.1.2",
    "qrcode.vue": "^3.3.3",
