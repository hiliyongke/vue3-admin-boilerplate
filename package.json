    "dom-to-image": "^2.6.0",
    "echarts": "^5.6.0",
    "file-saver": "^2.0.5",
    "generate-schema": "^2.6.0",
    "itools": "workspace:*",
    "lodash-es": "^4.17.21",
    "logger": "workspace:*",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "pinyin": "3.0.0-alpha.5",
    "plop": "^4.0.1",
    "qrcode.vue": "^3.6.0",
    "qs": "^6.14.0",
    "sortablejs": "^1.15.6",
    "splitpanes": "^4.0.4",
    "tdesign-icons-vue-next": "^0.3.6",
    "tdesign-vue-next": "^1.15.1",
    "tvision-color": "^1.6.0",
    "vconsole": "^3.15.1",
    "vue": "^3.5.18",
    "vue-clipboard3": "^2.0.0",
    "vue-pdf-embed": "^2.1.3",
    "vue-router": "^4.5.1",
    "vue3-pdfjs": "^0.1.6",
    "wangeditor": "^4.7.15",
    "xlsx": "^0.18.5"
