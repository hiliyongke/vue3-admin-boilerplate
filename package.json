    "@codemirror/language": "~6.11.2",
    "@codemirror/state": "~6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "~6.38.1",
    "@toast-ui/editor": "^3.2.2",
    "@vueuse/components": "^13.5.0",
    "@vueuse/core": "^13.5.0",
    "axios": "^1.11.0",
    "codemirror": "^6.0.2",
    "crypto-es": "^2.1.0",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.13",
