{
  "name": "vue3-admin",
  "version": "0.0.4",
  "author": "若尘",
  "homepage": "",
  "description": "a seed with vue standard boilerplate project",
  "main": "src/main.ts",
  "scripts": {
    "bootstrap": "pnpm install",
    "prepare": "husky install",
    "start": "pnpm run --filter '*' start",
    "dev": "vite --mode development",
    "dev:open": "vite --open --mode development",
    "build": "vue-tsc && vite build --mode production",
    "build:dev": "vue-tsc && vite build --mode development",
    "build:prod": "vue-tsc && vite build --mode production",
    "preview": "pnpm run build:dev && vite preview --host --open",
    "preview:prod": "pnpm run build && vite preview --host --open",
    "plop": "plop",
    "type-check": "vue-tsc --noEmit",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "lint:css": "stylelint \"src/**/*.{vue,scss,css,sass,less}\" --fix",
    "lint:css:check": "stylelint \"src/**/*.{vue,scss,css,sass,less}\"",
    "lint:prettier": "prettier --write \"src/**/*.{vue,js,jsx,ts,tsx,json,css,scss,less}\"",
    "lint:prettier:check": "prettier --check \"src/**/*.{vue,js,jsx,ts,tsx,json,css,scss,less}\"",
    "lint:file": "ls-lint",
    "lint:all": "pnpm run lint && pnpm run lint:css && pnpm run lint:prettier && pnpm run type-check",
    "lint-staged": "lint-staged -c .lintstagedrc.json",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "deps:update": "taze -w",
    "deps:update:major": "taze major -w",
    "deps:update:minor": "taze minor -w",
    "deps:update:patch": "taze patch -w",
    "deps:check": "taze",
    "commit": "cz",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:serve": "vitepress serve docs",
    "release": "standard-version",
    "release:cli": "node ./build/release.js",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "rimraf **/node_modules .eslintcache dist",
    "clean:deps": "rimraf node_modules pnpm-lock.yaml && pnpm install",
    "api:gen": "tsx ./src/utils/api-gen/index.ts"
  },
