{
  "name": "vue3-admin",
  "version": "0.0.4",
  "author": "若尘",
  "homepage": "",
  "description": "a seed with vue standard boilerplate project",
  "main": "src/main.ts",
  "scripts": {
    "bootstrap": "pnpm install",
    "prepare": "husky install",
    "start": "pnpm run --filter '*' start",
    "start:logger": "pnpm -C ./packages/logger start",
    "start:indexedDbHelper": "pnpm -C ./packages/IndexedDbHelper start",
    "start:itools": "pnpm -C ./packages/itools start",
    "dev": "vite --mode development",
    "dev:open": "vite --open --mode development",
    "build:dev": "vite build --mode development",
    "build:pro": "vite build --mode production",
    "preview": "npm run build:dev && vite preview --host --open",
    "plop": "plop",
    "typecheck": "npx vue-tsc --noEmit",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "lint:css": "stylelint  src/**/*.{vue,scss,css,sass,less} --fix",
    "lint:prettier": "prettier --write \"src/**/*.{vue,js,jsx,ts,tsx,json,css,scss,less}\"",
    "lint:file": "ls-lint",
    "lint-staged": "lint-staged -c .lintstagedrc.json",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:unit-coverage": "npx jest --coverage",
    "deps:update": "npx taze -w",
    "deps:update:major": "npx taze major -w",
    "deps:update:minor": "npx taze minor -w",
    "deps:update:patch": "npx taze patch -w",
    "commit": "cz",
    "docs:build": "npx vitepress build docs",
    "docs:dev": "npx vitepress dev docs",
    "docs:serve": "npx vitepress serve docs",
    "release": "standard-version",
    "release:cli": "node ./build/release.js",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "rimraf **/node_modules && rm -rf .eslintcache",
    "api:gen": "node ./src/utils/api-gen/index.ts"
  },
  "dependencies": {
